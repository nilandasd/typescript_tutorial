{
  "name": "typescript_practice",
  "version": "1.0.0",
  "description": "a simple social media app (to learn type script)",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "copy-assets": "ts-node tools/copyAssets",
    "lint": "tslint --project './src' -c tslint.json -p tsconfig.json --fix",
    "tsc": "tsc",
    "webpack": "npx webpack",
    "build": "npm-run-all clean lint tsc copy-assets webpack",
    "dev:start": "npm-run-all build debug:start",
    "dev": "nodemon --watch src -e ts,ejs --exec npm run dev:start",
    "debug:start": "node --inspect .",
    "start": "node .",
    "test": "jest --coverage"
  },
  "author": "lando",
  "license": "ISC",
  "dependencies": {
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "mongoose": "^8.0.0",
    "morgan": "^1.10.0",
    "selenium-webdriver": "^4.15.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.20",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^29.5.7",
    "@types/morgan": "^1.9.7",
    "@types/node": "^16.18.60",
    "@types/selenium-webdriver": "^4.1.19",
    "@types/shelljs": "^0.8.14",
    "@types/supertest": "^2.0.15",
    "css-loader": "^6.8.1",
    "dotenv": "^16.3.1",
    "fs-extra": "^10.1.0",
    "jest": "^29.7.0",
    "nodemon": "^2.0.22",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.5",
    "style-loader": "^3.3.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
